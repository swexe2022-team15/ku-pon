<!DOCTYPE html>
<html class="background1">
    
  <head>
    <title>Myapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  
  <body>
    
     <div id="container">
      
      <div id="header">
        <%= render "tops/header" %>
      </div>
      
      <div id="sidebar">
        <%= render "tops/sidebar" %>
      </div>
      
      <div id="content">
        <%= yield %>
      </div>
      
      <div id="footer">
        <%= render "tops/footer" %>
      </div>
      
    </div>
    
  </body>

</html>